<!DOCTYPE html>
  <html>
    <head>
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
      <link rel="stylesheet" type="text/css" href="./static">
      <link rel="stylesheet" type="text/css" href="./css">
      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
      <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
      <title>BAGZ</title>
    </head>
    <body id="home" class="scrollspy">
        <!-- Nav Bar -->
        <div class='navbar-fixed'>
            <nav class='black'>
                <div class='container'>
                    <div class='nav-wrapper'>
                        <a href='/'><img id='logo' style='height: 60px; width: 170px;'src='thebagz.png'></a>
                        <a href="#" data-target="mobile-nav" class="sidenav-trigger">
                            <i class="material-icons">menu</i>
                        </a>
                        <ul class='right hide-on-med-and-down'>
                            <li>
                            <a href="/" class='grey-text'>Home</a>
                            </li>
                            <li>
                            <a href="#home" class='grey-text'>Contact</a>
                            </li>
                            <li>
                            <a href="/signup" class='grey-text'>Sign Up</a>
                            </li>
                            <li>
                            <a href="/login" class='grey-text'>Log In</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
        <ul class='sidenav' id='mobile-nav'>
                <li>
                <a href="#home">Home</a>
                </li>
                <li>
                <a href="#home">Search</a>
                </li>
                <li>
                <a href="#home">Contact</a>
                </li>
                <li>
                <a href="#home">Sign Up</a>
                </li>
                <li>
                <a href="#home">Log In</a>
                </li>
            </ul>

            <!-- Section: Slider -->

            <div class="slider">
                <ul class="slides">
                    <li>
                    <img src="cloud1.jpg"> 
                     <div class="caption right-align black-text">
                     <h2>Traveling, simplified.</h2>
                     <h5 class="light black-text text-lighten-3">Bagz is the easiest and most convenient tool to help you travel the world with ease and comfort.</h5>
                     </div>
                    </li>
                     <li>
                     <img src="air2.jpg"> 
                     <div class="caption right-align black-text">
                     <h2>Request a driver</h2>
                     <h5 class="light black-text text-lighten-3">Our drivers are standing by to help take your bags to the airport.</h5>
                     </div>
                     </li>
                     <li>
                     <img src="air3.jpg"> 
                     <div class="caption right-align black-text">
                     <h2>Enjoy your day</h2>
                     <h5 class="light black-text text-lighten-3">While we safely transport your bags, enjoy your last day wherever you are!</h5>
                     </div>
                     </li>
                    </ul>
            </div>
            <!-- Section Search-->
            <div id='search' class='section section-search black darken-1 white-text center scrollspy'>
                <div class="container">
                    <div class="row">
                        <div class='col s12'>
                            <h3 class='grey-text'>Search Airports</h3>
                            <div class='input-field'>
                                <input type='text' class='white grey-text autocomplete' id='autocomplete-input' placeholder='LAX, JFK, MIA, etc..'>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Section Icon Boxes-->
            <div class='section section-icons grey lighten-4 center'>
                <div class='container'>
                    <div class='row'>
                        <div class='col s12 m4'>
                            <div class='card-panel'>
                                <i class='material-icons large green-text'>location_on</i>
                                <h4>Your Location</h4>
                                <p>Where are we picking up your bags from?</p>
                            </div>
                        </div>
                        <div class='col s12 m4'>
                                <div class='card-panel'>
                                    <i class='material-icons large green-text'>alarm</i>
                                    <h4>Pick Up Time</h4>
                                    <p>What time are we picking up your bags?</p>
                                </div>
                            </div>
                            <div class='col s12 m4'>
                                    <div class='card-panel'>
                                        <i class='material-icons large green-text'>airplanemode_active</i>
                                        <h4>Airport</h4>
                                        <p>Which airport are we taking them to?</p>
                                    </div>
                                </div>
                    </div>
                </div>
            </div>
            <!-- Section Follow-->
            <div class='section section-follow black darken-2 grey-text center'>
                <div class='container'>
                    <div class='row'>
                        <div class='col s12'>
                            <h4>Follow Bagz</h4>
                            <p>Follow us on social media for special offers and discount codes!</p>
                            <a href='#' class='grey-text'>
                                <i class='fab fa-facebook fa-4x'></i>
                            </a>
                            <a href='#' class='grey-text'>
                                <i class='fab fa-twitter fa-4x'></i>
                                </a>
                            <a href='#' class='grey-text'>
                                <i class='fab fa-linkedin fa-4x'></i>
                                </a>
                            <a href='#' class='grey-text'>
                                <i class='fab fa-snapchat fa-4x'></i>
                                </a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Section Contact-->
            <div id='contact' class='section section-contact scrollspy'>
                <div class='container'>
                    <div class='row'>
                        <div class='col s12 m6'>
                            <div class='card-panel green black-text center' style="border:solid black 2px;">
                                <i class='material-icons'>email</i>
                                <h5>Contact Us</h5>
                                <p>Questions? Send us an email! One of our customer service representatives will get back to you as soon as possible.</p>
                                <div class='card-panel green' style="border:solid black 2px;">

                                <form action='/send' method='POST'>
                                <div class='input-field'>
                                    <input type='text' placeholder='Full Name' name='name'>
                                </div>
                                <div class='input-field'>
                                        <input type='text' placeholder='Email' name='email'>
                                    </div>
                                <div class='input-field'>
                                    <textarea class='materialize-textarea' type='message' placeholder='Enter Message' name='message'></textarea>
                                </div>
                                <button class="btn waves-effect waves-light" type="submit">Submit
                                        <i class="material-icons right">send</i>
                                      </button>
                                </form>

                            </div>
                            </div>
                        </div>
                        <div class='col s12 m6'>
                            <div class='card-panel green' style="border:solid black 2px;">
                                <h5>Sign Up Here</h5>
                                <div class='input-field'>
                                    <input type='text' placeholder='First Name'>
                                </div>
                                <div class='input-field'>
                                    <input type='text' placeholder='Last Name'>
                                </div>
                                <div class='input-field'>
                                    <input type='number' placeholder='Phone Number'>
                                </div>
                                <div class='input-field'>
                                    <input type='text' placeholder='Email'>
                                </div>
                                <div class='input-field'>
                                    <input type='password' placeholder='Password'>
                                </div>
                                <!-- <div class='input-field'>
                                    <input type='password' placeholder='Confirm Password'>
                                </div> -->
                                    <input type="submit" value="Submit" class='btn'>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <footer class='section black darken-2 green-text center'>
                    <p class='flow-text'>Bagz &copy; 2018 </p>
                </footer>



      <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/js/materialize.min.js"></script>
      <script>

      // SideNav
      const sideNav = document.querySelector('.sidenav');
      M.Sidenav.init(sideNav, {});

      // Slider
      const slider = document.querySelector('.slider');
      M.Slider.init(slider, {
          indicators: false,
          height: 500,
          transition: 500,
          interval: 6000
      });

      // Auto-Complete
      const ac = document.querySelector('.autocomplete');
      M.Autocomplete.init(ac, {
          data: {
              "LAX": null,
              "JFK": null,
              "MIA": null,
              "LAX": null,
              "DFW": null,
              "LHR": null,
              "HKG": null,
              "SIN": null,
              "ORD": null,
              "ATL": null,
              "CDG": null,
              "ICN": null,
              "DXB": null,
              "PEK": null,
              "DEN": null,
              "MUC": null,
              "SFO": null,
              "FRA": null,
              "MAD": null,
          }
      });

      const ss = document.querySelectorAll('.scrollspy');
      M.ScrollSpy.init(ss, {});

      </script>
    </body>
  </html>